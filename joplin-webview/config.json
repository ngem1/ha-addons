{
  "name": "Joplin Vieweb",
  "version": "1.0.0",
  "slug": "joplin_vieweb",
  "description": "A web viewer/editor for Joplin notes",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "environment": {
    "ORIGINS": "*",
    "JOPLIN_LOGIN_REQUIRED": "False"
  }
}