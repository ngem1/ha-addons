{
  "name": "Joplin Vieweb",
  "version": "0.1.0",
  "slug": "joplin_vieweb",
  "description": "A web viewer/editor for Joplin notes",
  "startup": "services",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8080]/joplin",
  "ports": {
    "8080/tcp": 8080
  },
  "environment": {
    "ORIGINS": "http://localhost:8080",
    "JOPLIN_LOGIN_REQUIRED": "True"
  }
}
